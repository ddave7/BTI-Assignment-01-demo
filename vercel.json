{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node",
        "config": { "includeFiles": ["dist/**"] }
      }
    ],
    "routes": [
      
        {
            "src": "/",
            "dest": "server.js"
        },

        {
            "src": "/public.css",
            "dest": "/public/public.css"
        },

        {
            "src": "/about",
            "dest": "server.js"
        },

        {
        "src": "/(.*)",
        "dest": "server.js"
      }
    ]
  }